<Window x:Class="NetExtension.MVVM.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NetExtension.MVVM.Test"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Text="{Binding Name,Mode=TwoWay}"></TextBox>
            <Button Command="{Binding SetNameCommand}">设置名字</Button>
            <TextBox Text="11"></TextBox>
        </StackPanel>
    </Grid>
</Window>
